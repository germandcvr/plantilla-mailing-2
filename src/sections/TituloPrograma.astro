---
const { programa, config } = Astro.props
---
<span style=`text-transform: uppercase; color: ${config.colorPrincipal}; background-color: ${config.colorSecundario}; padding: 2px 2px; font-weight: bold; `>{programa.tipo}</span>
<h1 style=`text-transform: uppercase; font-size: 45px; font-weight: lighter; margin: 0; color: ${config.colorSecundario}`>
    {programa.titulo}
</h1>
<slot name="snies" />
